{
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/tests/**/*.js"]}],
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "class-methods-use-this": "off",
      "consistent-return": "off"
    }
  }
  